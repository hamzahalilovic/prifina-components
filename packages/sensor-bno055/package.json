{
  "name": "@prifina/sensor-bno055",
  "version": "0.0.19",
  "description": "Data connector for Sensor-BNO055 data",
  "author": "hamza <hamzahalilovic.12@gmail.com>",
  "homepage": "https://github.com/hamzahalilovic/prifina-components#readme",
  "license": "MIT",
  "main": "dist/cjs/packages/oura/src/index.js",
  "module": "dist/esm/packages/oura/src/index.js",
  "exports": {
    ".": "./dist/esm/packages/oura/src/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "rm -rf ./dist && rollup -c",
    "builds": "yarn build:cjs && yarn build:es && yarn build:esm",
    "build:esm": "cross-env NODE_ENV=production BABEL_ENV=esm babel ./src --out-dir ./dist/esm",
    "build:cjs": "cross-env NODE_ENV=production BABEL_ENV=cjs babel ./src --out-dir ./dist/cjs",
    "build:es": "cross-env NODE_ENV=production BABEL_ENV=es babel ./src --out-dir ./dist/es",
    "storybook": "start-storybook",
    "test": "./node_modules/.bin/jest"
  },
  "devDependencies": {
    "@babel/cli": "^7.16.8",
    "@babel/core": "^7.16.12",
    "@babel/preset-env": "^7.18.10",
    "@babel/preset-react": "^7.16.7",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.1.3",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-storyshots": "^6.0.16",
    "@storybook/react": "^6.0.16",
    "@styled-system/prop-types": "^5.1.5",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^12.1.2",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^27.5.1",
    "babel-loader": "^8.2.2",
    "babel-plugin-react-docgen": "^4.2.1",
    "core-js": "^3.24.1",
    "cross-env": "^7.0.3",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "jsdom": "20.0.0",
    "jsdom-global": "3.0.2",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "regenerator-runtime": "^0.13.9",
    "rollup": "^2.70.1"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "@dynamic-data/sensor-bno055-data": "^0.0.7"
  }
}
