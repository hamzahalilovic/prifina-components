    "@babel/preset-env": "^7.9.5",
    "@babel/preset-react": "^7.9.4",
    "@storybook/addon-actions": "^6.0.16",
    "@storybook/addon-storyshots": "^6.0.16",
    "@storybook/react": "^6.0.16",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-react-docgen": "^4.2.1",
    "cross-env": "^7.0.2",
    "react": ">=16.8.6",
    "react-dom": ">=16.8.6"
